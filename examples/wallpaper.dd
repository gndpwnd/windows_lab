DELAY 750
GUI r
DELAY 1000
STRING powershell Start-Process notepad -Verb runAs
ENTER
STRING $save_dir = "$HOME\Desktop\"
ENTER
DELAY 100
STRING $img_url = "https://i.ytimg.com/vi/3tleO8QtVTE/maxresdefault.jpg"
ENTER
DELAY 100
STRING $f = "rr.jpg"
ENTER
DELAY 100
STRING wget $img_url -O "$save_dir$f"
ENTER
DELAY 100
STRING reg add "HKEY_CURRENT_USER\Control Panel\Desktop" /v Wallpaper /t REG_SZ /d "$save_dir$f" /f
ENTER
DELAY 100
STRING Start-Sleep -s 3
ENTER
DELAY 100
